    "paint": {
      "fill-color": "#ddd",
      "fill-opacity": 0.3
    }
  },
  {
    "id": "greenery",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["in", "class", "wood", "agriculture", "cemetery", "grass", "park", "pitch"],
    "paint": {
      "fill-color": "#9bb57d",
      "fill-opacity": 0.3
    }
  },
    {
    "id": "hillshade",
    "type": "raster",
    "source": "mapbox",
    "layout": {
      "raster-size": 512,
      "raster-blur": 0.5
    },
    "paint": {
      "raster-opacity": {
        "stops": [ [13, 0.25], [16,0]]
      }
    },
    "layers": [{
      "id": "hill_full_highlight",
      "source": "mapbox",
      "source-layer": "hillshade",
      "filter": ["==", "class", "full_highlight"],
      "type": "fill",
      "paint": {
        "fill-color": "white",
        "fill-opacity": 1
      }
    }, {
      "id": "hill_medium_highlight",
      "source": "mapbox",
      "source-layer": "hillshade",
      "filter": ["==", "class", "medium_highlight"],
      "type": "fill",
      "paint": {
        "fill-color": "white",
        "fill-opacity": 0.8
      }
    }, {
      "id": "hill_full_shadow",
      "source": "mapbox",
      "source-layer": "hillshade",
      "filter": ["==", "class", "full_shadow"],
      "type": "fill",
      "paint": {
        "fill-color": "#008",
        "fill-opacity": 0.2
      }
    }, {
      "id": "hill_medium_shadow",
      "source": "mapbox",
      "source-layer": "hillshade",
      "filter": ["==", "class", "medium_shadow"],
      "type": "fill",
      "paint": {
        "fill-color": "#008",
        "fill-opacity": 0.2
      }
    }]
