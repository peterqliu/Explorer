{
  "version": 6,
  "sources": {
    "mapbox": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-terrain-v1,mapbox.mapbox-streets-v6-dev"
    }
  },
  "sprite": "sprite",
  "glyphs": "mapbox://fontstack/{fontstack}/{range}.pbf",
  "layers": [{
    "id": "land",
    "type": "background",
    "paint": {
      "background-color": "#f6f4e4",
      "background-opacity": 1
    }
  },
  {
    "id": "woods_&_forest",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landcover",
    "filter": ["in", "class", "wood"],
    "paint": {
      "fill-color": "#9bb57d",
      "fill-opacity": {
        "stops": [ [12, 0.2], [14,0]]
      }
    }
  },
  {
    "id": "scrub",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landcover",
    "filter": ["in", "class", "scrub"],
    "paint": {
      "fill-color": "#e0e8cd",
      "fill-opacity": {
        "stops": [ [12, 0.2], [14,0]]
      }    
    }
  },
  {
    "id": "crops_&_grass",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landcover",
    "filter": ["in", "class", "crop","grass"],
    "paint": {
      "fill-color": "#eeeed4",
      "fill-opacity": {
        "stops": [ [12, 0.2], [14,0]]
      }
    }
  },

  {
    "id": "parks_landuse_stroke",
    "type": "line",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["in", "class", "agriculture", "cemetery", "grass", "park", "pitch"],
    "paint": {
      "line-color": "#9bb57d",
      "line-width": 0,
      "line-opacity": 1
    }
  },
  {
    "id": "rocky_terrain",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["in", "class", "rock", "sand", "glacier"],
    "paint": {
      "fill-color": "gray",
      "fill-opacity": 0.2
    }
  },
{
    "id": "school",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["==", "class", "school"],
    "paint": {
      "fill-color": "#9ea3c2",
      "fill-opacity": 0.3
    }
  },
{
    "id": "airport",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["==", "class", "industrial"],
    "paint": {
      "fill-color": "#ddd",
      "fill-opacity": 0.3
    }
  }, 
{
    "id": "hospital",
    "type": "fill",
